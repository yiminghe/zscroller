    "pub": "rc-tools run pub --babel-runtime",
    "lint": "rc-tools run lint"
  },
  "devDependencies": {
    "pre-commit": "1.x",
    "rc-tools": "6.x",
    "react": "15.x",
    "react-dom": "15.x"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "babel-runtime": "6.x"
  }
